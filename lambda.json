{
  "name": "lambda_firestore_create_shopify_account",
  "description": "Save a shopify account in firestore",
  "region": "us-west-2",
  "runtime": "python2.7",
  "handler": "lambda_firestore_create_shopify_account.handler",
  "role": "{YOUR_LAMBDA_ROLE_ARN}",
  "requirements": ["google-cloud-firestore==0.28.0"],
  "ignore": [
    "circle\\.yml$",
    "\\.git$",
    "/.*\\.pyc$",
    "\\.lambda_uploader_env"
  ],
  "timeout": 300,
  "memory": 512,
  "tracing": {
    "Mode": "Active"
  }
}